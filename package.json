{
  "name": "instadapp-subgraph",
  "version": "0.1.0",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "format": "prettier --write \"./**/*.{js,jsx,json,ts}\"",
    "compile": "node scripts/compile_templates.js",
    "prepare:mainnet": "npm run compile mainnet && npm run codegen",
    "prepare:matic": "npm run compile matic && npm run codegen",
    "prepare:arbitrum": "npm run compile arbitrum && npm run codegen",
    "prepare:rinkeby": "npm run compile rinkeby && npm run codegen",
    "prepare:goerli": "npm run compile goerli && npm run codegen",
    "prepare:local": "npm run compile local && npm run codegen",
    "create:mainnet": "graph create stakeall/stakeall-finance-mainnet --node https://api.thegraph.com/deploy/ --access-token  ",
    "create:matic": "graph create niraj-kamdar/instadapp-polygon --node https://api.thegraph.com/deploy/",
    "create:local": "graph create niraj-kamdar/instadapp-local --node http://127.0.0.1:8020",
    "deploy:mainnet": "graph deploy stakeall/stakeall-finance-mainnet --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy:matic": "graph deploy cryptostaker2021/stakeall-finance-matic --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ ",
    "deploy:arbitrum": "graph deploy niraj-kamdar/instadapp-arbitrum --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy:local": "graph deploy niraj-kamdar/instadapp-local --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "deploy:studio:mainnet": "graph deploy --studio instadapp-mainnet",
    "deploy:rinkeyby": "graph deploy cryptostaker2021/stakeall-development --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/  --access-token 6890d9bc9b704b3ab0e7bc350955c4e0",
    "deploy:goerli": "graph deploy cryptostaker2021/stakeall-goerli --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --access-token 6890d9bc9b704b3ab0e7bc350955c4e0"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.20.1",
    "@graphprotocol/graph-ts": "^0.20.0",
    "pretier": "^0.0.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "nunjucks": "^3.2.3"
  }
}
